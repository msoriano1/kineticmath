<div class="bodynonav">
<fieldset>
<div id="log-in" class="container">
<h2>Log in | Admin </h2>
<br>
<br>
<div class="col-md-6 col-sm-3">
<%= simple_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="form-inputs">
    <div class="row">
    <div class="input-field">
    <%= f.label "Username" %>
    <%= f.input :ausername, required: true, autofocus: true, label: false, class: "form-control" %>
    </div>
    </div>
    <br>
    <div class="row">
    <div class="input-field">
    <%= f.label "Password" %>
    <%= f.input :password, required: true, :label => false,class: "form-control"  %>
    <%= f.input :remember_me, as: :boolean if devise_mapping.rememberable? %>
   </div>
   </div>
      </div>
      <br>
  <div class="form-actions">
    <%= f.button :submit, "Log in", class: "btn btn-primary btn-lg" %>
  </div>
<% end %>
 </div>
</div>
</fieldset>