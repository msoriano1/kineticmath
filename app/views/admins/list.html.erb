<br>
<h1> <strong>ACCOUNTS</strong>  |  Admin Accounts</h1>
<ol class="arrows">
    <li><%= link_to "Home", root_path, method: :get %></li>
    <li><h2>Accounts</h2></li>
</ol>
<div class="row">
    <div class="col-lg-12">
        <table class="table table-striped table-responsive-md">
            <thead>
                <tr>
                    <th>User Name</th>
                    <th style="width:80px"></th>
                    <th style="width:40px"></th>
                </tr>
            </thead>
            <tbody>
                <% @admins.each do |p| %>
                    <tr>
                        <td><%= p.ausername %></td>
                        <td><%= link_to "Edit",  admins_edit_path(admin: p.id), class: 'btn btn-warning'  %> </td>
                    </tr>
                <% end %>
            </tbody>
        </table> 
    </div>
</div>