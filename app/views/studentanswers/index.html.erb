<p id="notice"><%= notice %></p>

<h1>Listing Studentanswers</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @studentanswers.each do |studentanswer| %>
      <tr>
        <td><%= link_to 'Show', studentanswer %></td>
        <td><%= link_to 'Edit', edit_studentanswer_path(studentanswer) %></td>
        <td><%= link_to 'Destroy', studentanswer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Studentanswer', new_studentanswer_path %>
